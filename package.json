{
  "name": "dk-app",
  "version": "1.0.0",
  "description": "Dream Journal App - A React Native application for recording, analyzing, and tracking dreams with AI assistance. Features include mood analysis, dream interpretation, statistics, and intuitive navigation.",
  "main": "index.js",
  "author": "Cole Puls",
  "license": "MIT",
  "keywords": [
    "dream-journal",
    "react-native",
    "expo",
    "ai",
    "mood-analysis",
    "dream-interpretation",
    "mobile-app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/colepuls/dk-app.git"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    // Data persistence for storing dream entries locally
    "@react-native-async-storage/async-storage": "2.1.2",
    
    // Navigation libraries for tab and stack navigation
    "@react-navigation/bottom-tabs": "^7.4.1",
    "@react-navigation/native": "^7.1.13",
    "@react-navigation/native-stack": "^7.3.21",
    "@react-navigation/stack": "^7.3.6",
    
    // Testing utilities for component testing
    "@testing-library/react-native": "^13.2.0",
    
    // Expo framework and core modules
    "expo": "~53.0.12",
    "expo-av": "^15.1.6",           // Audio/Video capabilities for speech-to-text
    "expo-linear-gradient": "^14.1.5", // Gradient backgrounds and effects
    "expo-speech": "^13.1.7",        // Text-to-speech functionality
    "expo-status-bar": "~2.2.3",     // Status bar management
    
    // Icon library for consistent UI icons
    "lucide": "^0.519.0",
    "lucide-react-native": "^0.519.0",
    
    // Core React and React Native
    "react": "19.0.0",
    "react-native": "0.79.4",
    
    // Chart library for dream statistics and analytics
    "react-native-chart-kit": "^6.12.0",
    
    // Gesture handling for swipe navigation and interactions
    "react-native-gesture-handler": "~2.24.0",
    
    // Cryptography support for secure operations
    "react-native-get-random-values": "~1.11.0",
    
    // Animation library for smooth transitions and effects
    "react-native-reanimated": "~3.17.4",
    
    // Safe area handling for different device screens
    "react-native-safe-area-context": "5.4.0",
    
    // Screen management for navigation performance
    "react-native-screens": "~4.11.1",
    
    // SVG support for charts and custom graphics
    "react-native-svg": "15.11.2",
    
    // Router for web compatibility (if needed)
    "router-dom": "^3.0.3"
  },
  "devDependencies": {
    // Babel for JavaScript compilation and transformation
    "@babel/core": "^7.20.0",
    
    // Testing framework
    "jest": "^29.7.0",
    "jest-expo": "^53.0.0"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "private": true
}
