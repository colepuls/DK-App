{
  "name": "dk-app",
  "version": "1.0.0",
  "description": "Dream Journal App - A React Native application for recording, analyzing, and tracking dreams with AI assistance. Features include mood analysis, dream interpretation, statistics, and intuitive navigation.",
  "main": "index.js",
  "author": "Cole Puls",
  "license": "MIT",
  "keywords": [
    "dream-journal",
    "react-native",
    "expo",
    "ai",
    "mood-analysis",
    "dream-interpretation",
    "mobile-app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/colepuls/dk-app.git"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "setup-speech": "node setup-api-key.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.1",
    "@react-navigation/native": "^7.1.13",
    "@react-navigation/native-stack": "^7.3.21",
    "@react-navigation/stack": "^7.3.6",
    "@react-three/drei": "^10.3.0",
    "@react-three/fiber": "^9.1.2",
    "@testing-library/react-native": "^13.2.0",
    "expo": "~53.0.12",
    "expo-av": "^15.1.6",
    "expo-file-system": "^18.1.10",
    "expo-gl": "^15.1.6",
    "expo-gl-cpp": "^11.4.0",
    "expo-linear-gradient": "^14.1.5",
    "expo-speech": "~13.1.7",
    "expo-status-bar": "~2.2.3",
    "expo-three": "^8.0.0",
    "lucide": "^0.519.0",
    "lucide-react-native": "^0.519.0",
    "react": "19.0.0",
    "react-native": "0.79.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-webview": "^13.15.0",
    "router-dom": "^3.0.3",
    "three": "^0.177.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.0"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "private": true
}
