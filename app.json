{
  "expo": {
    "name": "Dreamio",
    "slug": "Dreamio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dkapp.dreamjournal",
      "buildNumber": "6",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs access to the microphone to enable voice input for dream journaling."
      },
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dkapp.dreamjournal",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-apple-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "9b278b20-2650-4003-9000-a3a627e39fa1"
      }
    },
    "owner": "yomonos"
  }
}
